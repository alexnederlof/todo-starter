{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "apollo-boost": "^0.1.27",
    "apollo-client": "^2.4.12",
    "graphql": "^14.1.1",
    "react": "^16.7.0",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.0.0",
    "typescript": "^3.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Start,Generate\" -c \"cyan.bold,green.bold\" \"yarn start\" \"yarn generate\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen --config codegen.yml --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@graphql-codegen/typescript": "1.1.1",
    "@graphql-codegen/typescript-operations": "1.1.1",
    "@graphql-codegen/typescript-react-apollo": "1.1.1",
    "@types/react": "^16.7.22",
    "@types/react-router-dom": "^4.3.1",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
