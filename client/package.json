{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "apollo-boost": "^0.4.2",
    "apollo-client": "^2.6.2",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "query-string": "^6.9.0",
    "react": "^16.7.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.7.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.3.0-next.62",
    "typeface-roboto": "^0.0.75",
    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Generate,Start\" -c \"magenta.bold,cyan.bold\" \"yarn generate --watch\" \"yarn start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@graphql-codegen/typescript": "^1.2.1",
    "@graphql-codegen/typescript-operations": "^1.17.10",
    "@graphql-codegen/typescript-react-apollo": "^1.2.1",
    "@types/react": "^16.8.20",
    "@types/react-router-dom": "^5.1.2",
    "concurrently": "^5.0.0"
  }
}
