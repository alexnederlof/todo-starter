{
  "name": "app",
  "version": "0.1.0",
  "main": "__generated__/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Generate,Start\" -c \"magenta.bold,cyan.bold\" \"yarn generate\" \"yarn start\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "generate": "graphql-codegen --config codegen.yml --watch",
    "postinstall": "expo-yarn-workspaces postinstall"
  },
  "dependencies": {
    "apollo-boost": "^0.4.3",
    "apollo-client": "^2.6.3",
    "common": "0.1.0",
    "expo": "^33.0.0",
    "expo-constants": "^5.0.1",
    "native-base": "^2.12.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-web": "^0.11.4",
    "react-navigation": "^3.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "^1.3.1",
    "@graphql-codegen/typescript-operations": "^1.3.1",
    "@graphql-codegen/typescript-react-apollo": "^1.3.1",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "babel-preset-expo": "^5.1.1",
    "concurrently": "^4.1.1",
    "expo-yarn-workspaces": "^1.2.0",
    "typescript": "^3.4.5"
  },
  "private": true
}
